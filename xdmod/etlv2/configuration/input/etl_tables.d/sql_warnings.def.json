{
    "#": "Test for warning messages generated by loading of incorrect integer, truncated, ",
    "#": "or out of range values in SQL statements ",

    "table_definition": {
        "name": "warning_test",
        "engine": "MyISAM",
        "comment": "Test SQL warnings",
        "columns": [
            {
                "name": "resource_id",
                "type": "int(11) unsigned",
                "nullable": false
            },
            {
                "name": "account_id",
                "type": "int(11) unsigned",
                "nullable": false,
                "extra": "auto_increment"
            },
            {
                "name": "provider_account_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "display",
                "type": "varchar(256)",
                "nullable": true,
                "default": null
            }
        ],

        "indexes": [
            {
                "name": "mykey",
                "columns": [ "account_id" ]
            }
        ]
    },

    "source_query": {
        "records": {
            "resource_id": -1,
            "account_id": "''",
            "provider_account_id": "''",
            "display": "'This is some random text'"
        },

        "joins": [
            {
                "name": "DUAL"
            }
        ]
    }
}

